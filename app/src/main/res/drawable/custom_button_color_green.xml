        android:bottomLeftRadius="@dimen/dimen_sp_corner_radius"
        android:bottomRightRadius="@dimen/dimen_sp_corner_radius"
        android:topLeftRadius="@dimen/dimen_sp_corner_radius"
        android:topRightRadius="@dimen/dimen_sp_corner_radius" />

</shape>